.row
  width 100%
  display block
  margin 0 auto
  word-wrap break-word
  box-sizing border-box

.col
  float left
  box-sizing border-box
  margin-right 1%

  &:last-child
    margin-right 0

.full
  width 100%

.half
  width 49%

.third
  width 32.333%

.fourth
  width 24%

.fifth
  width 19%

.o-fifth
  width 79%

.o-third
  width 65.333%
